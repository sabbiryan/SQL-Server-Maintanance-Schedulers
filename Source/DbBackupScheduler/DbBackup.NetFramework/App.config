	<configSections>
		<!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
		<section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
	</configSections>
	<connectionStrings>
		<add name="AzureStroage" connectionString="DefaultEndpointsProtocol=https;AccountName=yourAzureStorageAccountName;AccountKey=yourAzureStroageAccountKey;EndpointSuffix=core.windows.net" />
	</connectionStrings>
	<appSettings>
		<!--database server name-->
		<add key="ServerName" value=".\SQLDEVELOPER" />
		<!--backup all database of the provided server except master-->
		<add key="BackupAllDatabases" value="true" />
		<!--existing database names of the provided server-->
		<add key="BackupDatabases" value="AuditorDb,UrlShortenDb" />
		<!--backup will be stored on the application hosted base directory-->
		<add key="UseRootBackupDirectory" value="false" />
		<!--define a specific backup location. it will activate when UseRootBackupDirectory is false-->
		<add key="BackupDirectoryPath" value="C:\temp\backups\" />
		<!--remove older backup after n days, empty for disable this rule-->
		<add key="RemoveBackupAfterXDays" value="3" />
		<!--true for push the backup zip file to your azure storage blob-->
		<add key="PushToAzureStorage" value="false" />
	</appSettings>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>
	<entityFramework>
		<defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
		<providers>
			<provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
		</providers>
	</entityFramework>
